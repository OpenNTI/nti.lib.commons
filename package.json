{
  "name": "nti-commons",
  "version": "2.5.3",
  "author": "nextthought",
  "license": "UNLICENSED",
  "description": "Common utility javascript functions & classes.",
  "main": "lib/cjs-index.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.0"
  },
  "scripts": {
    "test": "make test",
    "start": "make check",
    "prepublish": "not-in-install && make || in-install",
    "bump": "npm version",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.lib.commons.git"
  },
  "publishConfig": {
    "registry": "https://npm.nextthought.com"
  },
  "dependencies": {
    "isempty": "0.0.1",
    "media-typer": "^0.3.0",
    "nti-lib-ntiids": "^1.0.0",
    "nti-util-logger": "^1.0.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-polyfill": "^6.1.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-1": "^6.1.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "cobertura"
    ],
    "testResultsProcessor": "./node_modules/jest-junit",
    "testMatch": [
      "**/__test__/**/*.(spec).js?(x)"
    ]
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}
